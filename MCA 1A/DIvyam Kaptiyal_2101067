<!DOCTYPE html>
<html>
  
<head>
    <script src=
"https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js">
    </script>
  
</head>
  
<body>
    <h1>Divyam Kaptiyal</h1>
  
    <div id="element">
        My name is Divyam Kaptiyal
    </div>
      
    <div class="button-container">
        <button id="click">
            hide
        </button>
    </div>
  
    <script>
        $('#click').on('click', function () {
            if ($('#click').text() === 'show') {
  
                // This block is executed when
                // you click the show button
                $('#click').text('hide');
                $('#element').show();
            }
            else {
  
                // This block is executed when
                // you click the hide button
                $('#click').text('show');
                $('#element').hide();
            }
        });
    </script>
</body>
  
</html>
